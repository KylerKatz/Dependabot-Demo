{
  "name": "dependabot-vuln-demo",
  "version": "1.0.0",
  "description": "A demo repo with outdated and vulnerable dependencies for Dependabot",
  "main": "index.js",
  "scripts": {
    "start": "node index.js"
  },
  "dependencies": {
    "express": "4.17.1",           // Outdated (latest ~4.19.x), has past vulns
    "lodash": "4.17.15",           // Vulnerable (e.g., CVE-2021-23337), latest is 4.17.21
    "minimist": "1.2.5",           // Vulnerable (CVE-2021-44906), latest is 1.2.8
    "moment": "2.24.0",            // Vulnerable (CVE-2022-31129), latest is 2.30.0
    "axios": "0.21.1",             // Outdated (latest ~1.7.x), had vulns in older versions
    "handlebars": "4.7.6",         // Vulnerable (CVE-2021-23369), latest is 4.7.8
    "jsonwebtoken": "8.5.1",       // Outdated (latest ~9.0.2), past vulns in 8.x
    "sanitize-html": "1.27.5"      // Vulnerable (CVE-2021-26540), latest is 2.13.0
  },
  "devDependencies": {
    "nodemon": "2.0.15",           // Outdated (latest ~3.1.x), minor past vulns
    "mocha": "8.4.0"               // Outdated (latest ~10.7.x), some older vulns
  }
}
